{
      "builds": [
        {
          "src": "package.json",
          "use": "@vercel/static-build",
          "config": { "distDir": "dist/rpgplace-angular/browser" }
        }
      ],
      "routes": [
        {
          "src": "/assets/(.*)",
          "dest": "/assets/$1"
        },
        {
          "src": "/(.*)",
          "dest": "/index.html"
        }
      ]
    }
    